{
  "name": "yup-by-example",
  "version": "3.0.2",
  "description": "A random, fake, contextual test data generator driven from Yup schemas",
  "main": "src/index.js",
  "scripts": {
    "coverage": "nyc mocha",
    "codeclimate": "(test -e .codeclimate && source .codeclimate && codeclimate-test-reporter < coverage/lcov.info) || echo skipping codeclimate",
    "doctoc": "doctoc README.md --github",
    "example": "node example/test/api.test.js",
    "lint": "eslint src test",
    "test": "mocha",
    "tdd": "mocha --watch",
    "qa": "npm run lint && npm run coverage && npm run example && npm run codeclimate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cressie176/yup-by-example.git"
  },
  "author": "@cressie176 Stephen Cresswell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cressie176/yup-by-example/issues"
  },
  "homepage": "https://cressie176.github.io/yup-by-example/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "pre-push": "npm run qa"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "mocha": {
    "require": "./test-setup"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mocha": "^6.3.0",
    "fast-stats": "^0.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "yup": "^0.29.3"
  },
  "dependencies": {
    "chance": "^1.1.4",
    "date-fns": "^2.13.0",
    "debug": "^4.1.1",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2"
  },
  "peerDependencies": {
    "yup": "^0.29.0"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "keywords": [
    "yup",
    "test",
    "data",
    "generator",
    "random",
    "fake",
    "fixtures",
    "contextual",
    "stub",
    "mock",
    "tdd",
    "schema"
  ],
  "nyc": {
    "all": false,
    "include": [
      "src/**/*.js"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ]
  }
}

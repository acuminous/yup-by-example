{
  "name": "yup-by-example",
  "version": "1.1.0",
  "description": "A random data generator driven from [Yup](https://github.com/jquense/yup) schemas",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc --reporter text-summary --reporter lcov --reporter html mocha",
    "tdd": "mocha --watch",
    "lint": "eslint src test",
    "qa": "npm run lint && npm run coverage",
    "precommit": "lint-staged"
  },
  "files": [
    "es",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cressie176/yup-by-example.git"
  },
  "author": "@cressie176 Stephen Cresswell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cressie176/yup-by-example/issues"
  },
  "homepage": "https://cressie176.github.io/yup-by-example/",
  "husky": {
    "hooks": {
      "pre-commit": "npm run qa",
      "pre-push": "npm run qa"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "mocha": {
    "require": "./test-setup"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mocha": "^6.3.0",
    "fast-stats": "^0.0.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "yup": "^0.28.5"
  },
  "dependencies": {
    "chance": "^1.1.4",
    "date-fns": "^2.13.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2"
  },
  "peerDependencies": {
    "yup": "^0.28.5"
  },
  "directories": {
    "example": "example",
    "test": "test"
  },
  "keywords": [
    "yup",
    "test",
    "data",
    "generator",
    "random",
    "schema"
  ]
}
